   "source": [
    "# Write a Python program to reverse a string."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "9e8ad991",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the string: \"1234abcd\"\n",
      "The reverse string is \"dcba4321\"\n"
     ]
    }
   ],
   "source": [
    "def reverse(string): \n",
    "    reverse = ''.join(reversed(string))\n",
    "    return reverse\n",
    "string = input('Enter the string: ')\n",
    "print('The reverse string is', reverse(string))"
   ]
